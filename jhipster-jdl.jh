entity Location{
	locationName String,
    image Blob,
    tag String,
    qrCode Blob,
    barCode Blob
}

entity Item{
	itemName String,
    image Blob,
    tag String,
    qrCode Blob,
    barCode Blob

}

relationship OneToMany{
	Location{item} to Item{car}
}


service all with serviceImpl
dto all with mapstruct
filter all